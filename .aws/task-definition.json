{
    "family": "task-definition", 
        "containerDefinitions": [
        {
            "name": "my-container",
            "image": "539967449544.dkr.ecr.ca-central-1.amazonaws.com/hello-app:latest",
            "cpu": 2,
            "memory": 8,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ],
            "essential": true,
            "entryPoint": [
                ""
